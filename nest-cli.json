{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "manualRestart": true,
    "deleteOutDir": true,
    "builder": {
      "type": "swc",
      "options": {
        "extensions": [
          ".js",
          ".ts",
          ".tsx",
          ".jsx"
        ]
      }
    },
    "typeCheck": true
  }
}